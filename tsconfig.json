{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "preserve",
    "types": ["vitest/globals", "@testing-library/jest-dom"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    // "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
